{
  "name": "allure-report-branch-action",
  "version": "0.0.1",
  "description": "Allure Report with history per branch",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate_reports": "node prebuild.js",
    "build": "npm run generate_reports && ncc build index.ts --target es2021 --license licenses.txt",
    "compile": "npm run generate_reports && tsc",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "precommit": "run-p compile lint",
    "postinstall": "npm run generate_reports"
  },
  "keywords": ["github", "action", "generate", "allure", "history","branch"],
  "author": "mgrybyk",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^16.18.39",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
